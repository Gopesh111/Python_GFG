class Solution:
    def findElement(self, arr):
        # code here
        n=len(arr)
        if(n<3):
            return -1
        l_max=[0]*n
        r_min=[0]*n
        l_max[0]=arr[0]
        for i in range(1,n):
            l_max[i]=max(l_max[i-1],arr[i])
        r_min[n-1]=arr[n-1]
        for i in range(n-2,-1,-1):
            r_min[i]=min(r_min[i+1],arr[i])
        for i in range(1,n-1):
            if l_max[i-1]<arr[i]<r_min[i+1]:
                return arr[i]
        return -1
