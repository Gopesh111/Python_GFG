class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s)!=len(t):
            return False
        else:
            ds={}
            dt={}
            for char in s:
                ds[char]=ds.get(char,0)+1
            for char in t:
                dt[char]=dt.get(char,0)+1
            return ds==dt
            

