class Solution:
    def countLessEq(self, a, b):
        # code here
        res=[]
        b.sort()
        for x in a:
            low,high=0,len(b)-1
            ans=-1
            while(low<=high):
                mid=(low+high)//2
                if(b[mid]<=x):
                    ans=mid
                    low=mid+1
                else:
                    high=mid-1
            res.append(ans+1)
        return res
        
