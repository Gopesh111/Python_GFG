class Solution:
    def twoRepeated(self, arr):
        result = []
        
        for i in range(len(arr)):
            idx = abs(arr[i]) - 1
            
            if arr[idx] < 0:
                result.append(abs(arr[i]))
                if len(result) == 2:
                    return result
            else:
                arr[idx] = -arr[idx]
        
        return result
